{
  "name": "mean-starter",
  "version": "0.0.1",
  "description": "MEAN starter project based on hackathon hacker starter file",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "export NODE_ENV=development || SET NODE_ENV=development && nodemon --exec npm start",
    "test": "export NODE_ENV=test || SET NODE_ENV=test && nyc mocha *.test.js --timeout=1000 --exit",
    "watch-test": "nodemon --exec npm test",
    "lint": "eslint .",
    "lint-setup": "eslint -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmfranciacastillo/mean-starter.git"
  },
  "keywords": [
    "mongodb",
    "angular",
    "node",
    "express"
  ],
  "author": "Renato Francia",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rmfranciacastillo/mean-starter/issues"
  },
  "homepage": "https://github.com/rmfranciacastillo/mean-starter#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^8.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  },
  "lint-staged": {
    "server.js": [
      "eslint --fix",
      "git add"
    ],
    "controllers/*.js": [
      "eslint --fix",
      "git add"
    ],
    "config/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "chalk": "^2.4.2",
    "chart.js": "^2.8.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-status-monitor": "^1.2.3",
    "jquery": "^3.3.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.1",
    "morgan": "^1.9.1",
    "node-sass": "^4.11.0",
    "node-sass-middleware": "^0.11.0",
    "path": "^0.12.7",
    "popper.js": "^1.14.7",
    "pug": "^2.0.3",
    "request": "^2.88.0"
  }
}
